{
  "name": "amazin",
  "description": "Creates links to local Amazon instances by GeoIP lookup",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "deploy:staging": "npm run build && now --public --target staging",
    "deploy:prod": "npm run build && now --public --target production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/donovanh/amazin"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "geoip-lite": "^1.3.7",
    "maxmind": "^2.12.0",
    "polka": "^0.5.0",
    "request-ip": "^2.1.3",
    "sirv": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "chokidar": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "alpha",
    "svelte": "^3.0.0"
  }
}
